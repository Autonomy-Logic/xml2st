FUNCTION_BLOCK {{name}}
    VAR_INPUT
        {%-for k,v in vars.items()%}
        {%- if v.value is not none %}
        {{k}} : {{v.type}} := {{v.value}};
        {%- else %}
        {{k}} : {{v.type}};
        {%- endif %}
        {%- endfor %}
    END_VAR
    VAR
        local_temp : DINT;
    END_VAR
    
    local_temp := 0;
END_FUNCTION_BLOCK